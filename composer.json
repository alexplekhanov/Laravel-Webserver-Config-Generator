{
    "name": "alexplekhanov/laravel-webserver-config-generator",
    "description": "Webserver config generator for Laravel projects",
    "keywords": ["webserver", "Laravel", "config"],
    "license": "MIT",
    "support": {
    },
    "authors": [
        {
            "name": "Alexey Plekhanov",
            "email": "alex@plekhanov.us"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel-zero/framework": "~3.6"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "post-create-project-cmd": [
            "php application app:rename"
        ]
    },
    "bin": ["lwcg"]
}
